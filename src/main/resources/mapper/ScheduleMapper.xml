        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boyi.hospital.mapper.ScheduleMapper">

    <resultMap id="BaseResultMap" type="com.boyi.hospital.model.entity.Schedule">
